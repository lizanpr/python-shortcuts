# THIS IS A LITTLE CLASS FOR CAPTURING OUTPUT TO BOTH A FILE AND TERMINAL

import sys

class Logger(object):
    def __init__(self):
        self.terminal = sys.stdout
        self.log = open("logfile.log", "a")

    def write(self, message):
        self.terminal.write(message)
        self.log.write(message) 

sys.stdout = Logger()
